{
  "name": "directory-fixture-provider",
  "version": "1.6.37",
  "description": "Provides directories for testing.",
  "main": "dist/src/index.js",
  "scripts": {
    "test-dev": "node_modules/.bin/mocha --recursive tests/tests",
    "test-dev-unit": "npm run clean && node_modules/.bin/mocha --recursive tests/tests/unit",
    "test-dev-coverage": "node_modules/.bin/mocha --recursive tests/tests",
    "test-old": "npm run build && npm run test-compiled && npm run clean",
    "test-project": "pwd && npm run license-check && node_modules/.bin/depcheck && npm run standard-fix",
    "test": "pwd; mkdir tmp;PROD=true node_modules/.bin/nyc --reporter=lcov --reporter=clover --reporter=text --cache npm run test-dev-coverage",
    "test-all": "npm run test-project && npm test",
    "test-all-dev": "npm run test-project && npm run test-dev",
    "test-compiled": "node_modules/.bin/mocha --require babel-core/register --require babel-polyfill --recursive dist/tests/tests",
    "build-src": "mkdir tmp; rm -rf dist;mkdir dist;mkdir dist/src;node_modules/babel-cli/bin/babel.js src --presets babel-preset-env --out-dir dist/src",
    "build-test": "mkdir dist/tests; node_modules/babel-cli/bin/babel.js tests --presets babel-preset-env --out-dir dist/tests",
    "prepublish": "npm run build-src && npm run build-test",
    "clean": "rm -rf tests/tmp; rm -rf dist;",
    "standard-fix": "node_modules/.bin/standard --fix src/index.js src/**/*.js test/**/*.js",
    "license-check": "node_modules/.bin/license-checker --failOn unknown --summary"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vidaxl-com/cowlog/tree/master/packages/directory-fixture-provider"
  },
  "keywords": [
    "testing",
    "tests",
    "directory",
    "development",
    "tools",
    "tooling",
    "cowlog",
    "directory",
    "fixture",
    "fixtures"
  ],
  "author": "Imre Toth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imre-vidaxl/directory-fixture-provider/issues"
  },
  "homepage": "https://github.com/vidaxl-com/cowlog/tree/master/packages/directory-fixture-provider",
  "dependencies": {
    "directory-exists": "^2.0.0",
    "dsl-framework": "^1.6.37",
    "empty-dir": "^2.0.0",
    "fs-extra": "^7.0.0",
    "is-empty": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pkg-dir": "^4.0.0",
    "randomstring": "^1.1.5",
    "recursive-readdir-sync": "^1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "depcheck": "^0.8.0",
    "is-directory": "^0.3.1",
    "license-checker": "^25.0.0",
    "mocha": "6.1.4",
    "nyc": "^14.0.0",
    "shelljs": "^0.8.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": "<=6.4.0"
  },
  "nyc": {
    "exclude": [
      "**/tests/**"
    ]
  },
  "standard": {
    "ignore": [
      "/dist",
      "/coverage/",
      "/build/",
      "/tmp/"
    ]
  },
  "files": [
    "dist/src/**"
  ],
  "gitHead": "d078870b73cc2d73e0c9050f1e20896bdb4b7220"
}
